{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "yarn build",
  "cleanUrls": true,
  "installCommand": "yarn install",
  "outputDirectory": ".vercel/output",
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    },
    "api/properties/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  }
}
